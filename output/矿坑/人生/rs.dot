digraph Story {
    rankdir=TB;
    splines=ortho;
    nodesep=0.6;
    ranksep=0.8;

    node [shape=box, style=filled, fontcolor=white, fontname="Microsoft YaHei"];
    edge [fontname="Microsoft YaHei"];

    "node_01" [label="出生\n[node_01]", fillcolor="#4CAF50", border="none"];
    "node_02" [label="长大\n[node_02]", fillcolor="#4CAF50", border="none"];
    "node_03" [label="结婚\n[node_03]", fillcolor="#2196F3", border="none"];
    "node_04" [label="离婚\n[node_04]", fillcolor="#2196F3", border="none"];
    "node_05" [label="死亡\n[node_05]", fillcolor="#4CAF50", border="none"];

    "node_01" -> "node_02" [color="#333333"];
    "node_02" -> "node_05" [color="#333333"];
    "node_03" -> "node_04" [color="#333333"];
    "node_04" -> "node_05" [color="#333333"];

    "node_02" -> "node_03" [label="谈恋爱", color="#FF9800", fontcolor="#FF9800"];
}